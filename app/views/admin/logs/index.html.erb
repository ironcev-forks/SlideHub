<section class="content-header">
<h1><%= t(:logs) %><small></small></h1>
<ol class="breadcrumb">
  <li><a href="<%= admin_dashboard_path %>"><i class="fa fa-dashboard"></i> <%= t(:admin_dashboard) %></a></li>
  <li class="active"><%= link_to t(:logs), admin_logs_index_path %></li>
</ol>
</section>

<section class="content">
<%= render :partial => 'shared/flash', :locals => { :flash => flash } %>

<div class="box box-danger">
  <!-- Table -->
  <table class="table table-striped table-bordered small">
    <thead>
      <tr>
        <th class="col-md-12"><%= t(:path) %></th>
      </tr>
    </thead>
    <tbody>
      <% @files.each do |f| %>
      <tr>
        <td><%= link_to f, "#{admin_logs_show_path}?path=#{f}" %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
</section>
